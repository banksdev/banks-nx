{
  "name": "banks-micro-service",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/banks-micro-service/src",
  "targets": {
    "serve": {
      "executor": "@ns3/nx-serverless:sls",
      "options": {
        "command": "offline"
      }
    },
    "start": {
      "command": "serverless offline start"
    },
    "package": {
      "executor": "@ns3/nx-serverless:sls",
      "outputs": [
        "apps/banks-micro-service/.serverless",
        "dist/apps/banks-micro-service"
      ],
      "dependsOn": ["^build"],
      "options": {
        "command": "package"
      }
    },
    "deploy": {
      "executor": "@ns3/nx-serverless:sls",
      "outputs": [
        "apps/banks-micro-service/.serverless",
        "dist/apps/banks-micro-service"
      ],
      "dependsOn": ["package"],
      "options": {
        "command": "deploy",
        "package": ".serverless"
      }
    },
    "remove": {
      "executor": "@ns3/nx-serverless:sls",
      "options": {
        "command": "remove"
      }
    },
    "sls": {
      "executor": "@ns3/nx-serverless:sls",
      "options": {}
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/banks-micro-service/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/banks-micro-service/src/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
